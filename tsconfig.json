{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "skipLibCheck": true // Add this to avoid issues with type definitions
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "dist"] // Exclude the output directory
}